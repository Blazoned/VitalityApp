
@{
    ViewData["Title"] = "Rust Invoeren";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"></script>
<link rel="stylesheet" type="text/css" href="~/css/clockpicker.css">

<script src="js/jquery-clockpicker.js"></script>
<script src="js/jquery-clockpicker.min.js"></script>


<div class="container row">
    <div class="form-group block-wrapper col-xs-12 col-md-4">
        <a class="btn btn-primary backBtnGroup2" role="button" href="~/Movement/Index">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <br/>
        <br/>
        <h1><span class="label label-default">Rust Invoeren</span></h1>
        <br/>
        <div class="input-group clockpicker">
            <input type="text" class="form-control" placeholder="Begintijd">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-time"></span>
            </span>
        </div>
        <br/>
        <div class="input-group clockpicker">
            <input type="text" class="form-control" placeholder="Eindtijd">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-time"></span>
            </span>
        </div>
    </div>
</div>




<script type="text/javascript">
    $('.clockpicker').clockpicker();
</script>